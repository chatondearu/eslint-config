{
  "name": "@chatondearu/eslint-config",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.13.1",
  "description": "My Eslint config",
  "author": "ChatonDeAru <hi@chatondearu.fr>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chatondearu",
  "homepage": "https://github.com/chatondearu/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatondearu/eslint-config.git"
  },
  "bugs": "https://github.com/chatondearu/eslint-config/issues",
  "keywords": [
    "eslint",
    "eslint-config",
    "chatondearu"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^9.22.0",
    "@antfu/eslint-config": "^4.16.2"
  },
  "dependencies": {
    "@antfu/eslint-config": "catalog:prod",
    "eslint": "catalog:prod",
    "eslint-flat-config-utils": "catalog:prod"
  },
  "devDependencies": {
    "@antfu/ni": "catalog:cli",
    "@types/node": "catalog:types",
    "bumpp": "catalog:cli",
    "lint-staged": "catalog:cli",
    "simple-git-hooks": "catalog:cli",
    "tinyexec": "catalog:utils",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "unbuild": "catalog:cli",
    "vite": "catalog:cli",
    "vitest": "catalog:testing",
    "vitest-package-exports": "catalog:testing",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
